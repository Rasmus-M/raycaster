**
* Door structure
*
door_x:
       equ  0
door_y:
       equ  2
door_n_objects:
       equ  4
door_object_inits:
       equ  6
door_size:
       equ  8

door_1:
       data >3700
       data >3700
       data 1
       data door_1_init
door_2:
       data >1400
       data >3a00
       data 1
       data door_2_init
door_3:
       data >0400
       data >0e00
       data 2
       data door_3_init
door_4:
       data >0800
       data >1c00
       data 2
       data door_4_init
door_5:
       data >2800
       data >1c00
       data 2
       data door_5_init
door_6:
       data >0800
       data >3d00
       data 1
       data door_6_7_init
door_7:
       data >0100
       data >3a00
       data 1
       data door_6_7_init
n_doors:
       data (n_doors - doors) / door_size

object_init_data:
