       data >2f00                      ; y
       data object_type_guard
       data >3900                      ; x
       data >0400                      ; y
door_1_init:
       data object_type_guard
       data >3400                      ; x
       data >3c00                      ; y
door_2_init:
       data object_type_guard
       data >1b00                      ; x
       data >3d00                      ; y
door_3_init:
       data object_type_guard
       data >0100                      ; x
       data >1000                      ; y
       data object_type_chest
       data >0e00                      ; x
       data >1100                      ; y
door_4_init:
       data object_type_guard
       data >0400                      ; x
       data >1800                      ; y
       data object_type_guard
       data >0400                      ; x
       data >2000                      ; y
door_5_init:
       data object_type_guard
       data >2700                      ; x
       data >1800                      ; y
       data object_type_guard
       data >2900                      ; x
       data >1800                      ; y
door_6_7_init:
